{
    "name": "it_premium/tecdoc",
    "description": "TecDoc module for PrestaShop. This module integrates with the official TecAlliance TecDoc API.",
    "license": "Single Domain License",
    "authors": [
        {
            "name": "IT PREMIUM OU",
            "email": "info@itpremium.net",
            "homepage": "https://itpremium.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "ItPremium\\TecDoc\\": "src/"
        },
        "classmap": [
            "classes/",
            "controllers/"
        ]
    },
    "config": {
        "prepend-autoloader": false
    },
    "type": "prestashop-module",
    "require": {
        "php": ">=8.1",
        "cuyz/valinor": "1.4",
        "phpoffice/phpspreadsheet": "1.28",
        "google/recaptcha": "^1.3",
        "k-samuel/faceted-search": "^3.1",
        "kevinrob/guzzle-cache-middleware": "^4.1"
    },
    "require-dev": {
        "prestashop/autoindex": "^2.1",
        "prestashop/php-dev-tools": "^4.3"
    },
    "author": "PrestaShop"
}
